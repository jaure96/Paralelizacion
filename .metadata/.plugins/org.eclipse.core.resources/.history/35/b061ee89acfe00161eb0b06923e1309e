package lock;

import java.util.concurrent.locks.Lock;
import java.util.concurrent.locks.ReentrantLock;

public class Cuenta {
	
	long valor;
	Lock lock;
	
	public Cuenta() {
		valor = 0;
		lock = new ReentrantLock();
	}


	public void sumarValor(){
		lock.lock();
		this.valor++;
		lock.unlock();
	}
	
	

}
